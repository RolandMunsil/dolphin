{
    "rules":
    {
        "class-name": true,
        "curly": true,
        "member-access": true,
        "no-any": true,
        "no-duplicate-switch-case": true,
        "no-duplicate-variable": true,
        "no-for-in-array": true,
        "no-unnecessary-type-assertion": true,
        "no-var-keyword": true,
        "prefer-const": true,
        "prefer-for-of": true,
        "semicolon": [true, "always"],
        "switch-default": true,
        "triple-equals": true,
        "use-isnan": true
    }
}